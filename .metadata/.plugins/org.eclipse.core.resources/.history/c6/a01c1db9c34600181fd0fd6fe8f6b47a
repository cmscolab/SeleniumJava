package advanced;



import org.testng.annotations.DataProvider;

import wdMethodsold.ProjectMethods;

public class CreateLead{

	
	public static void main(String[] args){
		int x = 1;
		int y = 2;
		int z = 3;
		
		x |= 4;
		y >>= 1;
		z<<=1;
		x^=z;
		 System.out.println(x + " "+ y + " "+ z);
		}

}
